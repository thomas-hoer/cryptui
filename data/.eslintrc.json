{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-absolute-path": "off"
    },
    "globals": {
        "wasm": "readonly",
        "Go": "readonly"
    }
}
