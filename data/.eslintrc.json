{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-absolute-path": "off"
    },
    "globals": {
        "wasmDecryptToString": "readonly",
        "wasmDecryptToBase64": "readonly",
        "wasmEncryptString": "readonly",
        "wasmEncrypt": "readonly",
        "wasmCreateKey": "readonly",
        "encryptAES": "readonly",
        "decryptAES": "readonly",
        "signFile": "readonly",
        "Go": "readonly"
    }
}
